{
  "name": "@greenCheck/greencheck-module",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": "./src/*"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "./*": "./dist/*"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "@greenCheck/gh-diff": "*",
    "@elastic/elasticsearch": "9",
    "@langchain/core": "^0.3.72",
    "@langchain/langgraph": "^0.4.6",
    "@langchain/openai": "^0.6.9",
    "dotenv": "^17.2.1",
    "lodash": "^4.17.21",
    "parse-git-patch": "^2.1.1",
    "simple-git": "^3.28.0",
    "typescript": "^5.4.5",
    "yargs": "^17.7.2",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@greenCheck/typescript-config": "*",
    "@types/node": "^22",
    "tsup": "^8.0.2",
    "vitest": "3.1.4"
  },
  "scripts": {
    "build": "tsup src/index.ts",
    "dev": "tsup src/index.ts --watch",
    "check-types": "tsup --dts-only",
    "test": "vitest"
  }
}
